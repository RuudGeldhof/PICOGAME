pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
playerx = 64
playery = 64
velx = 0
vely = 0
grounded = false
gravity = 0.5

function _update()
  if (btn(0) and playerx > 0) playerx -= 1
  if (btn(1) and playerx < 127) playerx += 1
  if (btn(2) and playery > 0 and grounded) vely -= 1
  if (btn(3) and playery < 127) playery += 1
dogravity()  
checkcollission()

playery += vely
end

function _draw()
	cls()
	spr(1,playerx,playery)
	print(grounded)
	map(0,0,0,0,16,16)
end

function checkcollission()
	cellx = flr(playerx / 8)
	celly = flr(playery / 8)
	cellbelow = mget(cellx,celly+1)
	if fget(cellbelow,0) then
		grounded = true
	else
		grounded = false
	end
end

function dogravity()
	if not grounded then
		vely += gravity
	elseif vely > 0 then
		vely = 0
	end
end
